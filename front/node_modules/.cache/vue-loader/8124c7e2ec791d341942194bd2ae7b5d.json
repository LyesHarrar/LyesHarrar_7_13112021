  fetch(url, options)\n                .then(res => res.json())\n                .then(res => {\n                    if(res.ok) {\n                        this.postInfo = {}\n                    }\n                    this.updateMessage = res.message;\n                    this.published = true;\n                })\n            .catch(error => {\n                this.errorMessage = error\n            })\n        }\n    }\n}\n</script>\n\n<style lang=\"css\">\n@import '../style/style.css'\n</style>"]}]}